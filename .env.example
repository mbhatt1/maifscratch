# MAIF Production Environment Variables

# Application Settings
MAIF_APP_NAME=MAIF
MAIF_ENVIRONMENT=production
MAIF_DEBUG=false
MAIF_LOG_LEVEL=INFO

# AWS Configuration
AWS_DEFAULT_REGION=us-east-1
AWS_PROFILE=
MAIF_S3_BUCKET=maif-artifacts
MAIF_DYNAMODB_TABLE=maif-metadata
MAIF_KMS_KEY_ID=

# Performance Settings
MAIF_MAX_WORKERS=10
MAIF_BATCH_SIZE=100
MAIF_CONNECTION_POOL_SIZE=10
MAIF_REQUEST_TIMEOUT=30

# Rate Limiting
MAIF_RATE_LIMIT_ENABLED=true
MAIF_REQUESTS_PER_SECOND=100.0
MAIF_BURST_SIZE=200

# Cost Tracking
MAIF_COST_TRACKING_ENABLED=true
MAIF_BUDGET_LIMIT=1000.0
MAIF_BUDGET_PERIOD=monthly

# Monitoring
MAIF_METRICS_ENABLED=true
MAIF_METRICS_NAMESPACE=MAIF/Production
MAIF_HEALTH_CHECK_INTERVAL=30

# Security
MAIF_ENABLE_ENCRYPTION=true
MAIF_ENABLE_SIGNING=true
MAIF_ENABLE_ACCESS_CONTROL=true

# Retry Configuration
MAIF_MAX_RETRIES=3
MAIF_RETRY_BASE_DELAY=1.0
MAIF_RETRY_MAX_DELAY=60.0

# Cache Settings
MAIF_CACHE_ENABLED=true
MAIF_CACHE_TTL=3600

# Database URLs (if using external databases)
# DATABASE_URL=postgresql://user:password@host:port/dbname
# REDIS_URL=redis://localhost:6379/0

# API Keys (store securely in production)
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
# COHERE_API_KEY=

# Monitoring and Alerting
# SENTRY_DSN=
# DATADOG_API_KEY=
# PAGERDUTY_API_KEY=

# Feature Flags
# FEATURE_NEW_UI=false
# FEATURE_BETA_MODELS=false